<%= form_with(model: [ @project, @project.bugs.build ], local: true) do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title%>
  </p>
  <p>
    <%= form.label :description%><br>
    <%= form.text_area :body%>
  </p>
 

    <% class_types = ["New","Started","Completed"] %>
  <div class ="field">
    <%= form.label :status, 'Status:' %>
    <%= form.select(:status, class_types,{}) %>
  </div>

  <p>
    <%= form.submit %>
  </p>
<% end %>